<!doctype html>
<html lang="en">
  <head>
    {% render 'meta-tags' %}
    {% render 'meta-tags-social' %}
    <link href="{{ 'favicon.png' | asset_url }}" rel="icon" type="image/x-icon">
    {{ content_for_header }}
    <link href="//fonts.googleapis.com" rel="preconnect">
    <link href="//fonts.gstatic.com" rel="preconnect" crossorigin>
    <link href="//fonts.googleapis.com/css2?family=Poppins:ital,wght@0,300;0,600;1,300;1,600&display=swap" rel="stylesheet">
    <link href="//cdn.jsdelivr.net/npm/@fancyapps/ui@4.0/dist/fancybox.css" rel="stylesheet">
    <link href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" rel="stylesheet" type="text/css">
    {{ 'application.css' | asset_url | stylesheet_tag }}
    {% if settings.sticky == true %}
    <style>header { position: sticky; top: 0; z-index: 9; }</style>
    {% endif %}
  </head>
  <body class="page-{{ template | handleize }}">
    {% render 'nav-mobile' %}
    {% render 'cart-drawer' %}
    {% render "search-box" %}
    {% section "header" %}
    <main>
      <div class="wrap">
      {{ content_for_layout }}
      </div>
    </main>
    {% section "footer" %}
    <script src="//code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/@fancyapps/ui@4.0/dist/fancybox.umd.js"></script>
    <script src="//cdn.jsdelivr.net/npm/jquery.scrollto@2.1.3/jquery.scrollTo.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    {{ 'predictive-search.js' | asset_url | script_tag }}
    {{ 'theme-addresses.js' | asset_url | script_tag }}
    {{ 'application.js' | asset_url | script_tag }}
  </body>
</html>
  